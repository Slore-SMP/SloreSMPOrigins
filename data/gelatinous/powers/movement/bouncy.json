{
    "type": "origins:multiple",
    "name": "Bouncy",
    "description": "You jump very high, and you take and deal more knockback. You're immune to fall damage, and bounce from landing.",
    "badges": [
        {
            "type": "origins:tooltip",
            "sprite": "origins:textures/gui/badge/info.png",
            "text": "You do not bounce while sneaking or jumping. At low heights, you do not bounce while sprinting."
        }
    ],
    "jump": {
        "type": "origins:modify_jump",
        "modifier": {
            "operation": "add_base_late",
            "value": 0.153
        },
        "entity_action": {
            "type": "origins:delay",
            "ticks": 2,
            "action": {
                "type": "origins:if_else",
                "if_action": {
                    "type": "origins:and",
                    "actions": [
                        {
                            "type": "origins:execute_command",
                            "command": "playsound minecraft:entity.slime.jump player @a[distance=..10] ~ ~ ~ 2 1.5 1"
                        },
                        {
                            "type": "origins:execute_command",
                            "command": "particle minecraft:item_slime ~ ~ ~ 0.3 0.1 0.3 0.1 10 normal @a"
                        }
                    ]
                },
                "condition": {
                    "type": "origins:on_block"
                }
            }
        }
    },
    "fall_damage_nullification": {
