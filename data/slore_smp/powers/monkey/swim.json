{
	"type": "origins:multiple",
	"name": "Non-Aquatic",
	"description": "You can't swim in water, and you have 33% less oxygen",
	"sprint": {
		"type": "origins:prevent_sprinting",
		"condition": {
			"type": "origins:submerged_in",
			"fluid": "water"
		}
	},
	"speed": {
		"type": "origins:modify_swim_speed",
		"modifier": {
			"operation": "multiply_total",
			"value": 0.5
		},
		"condition": {
			"type": "origins:or",
			"conditions": [
			{
				"type": "origins:fluid_height",
				"fluid": "water",
				"comparison": ">",
				"compare_to": 0
			},
			{
				"type": "origins:fluid_height",
				"fluid": "lava",
				"comparison": ">",
				"compare_to": 0
			}
			]
		}
	},
	"breath": {
		"type": "origins:action_over_time",
		"interval": 20,
		"condition": {
			"type": "origins:and",
			"conditions": [
			{
				"type": "origins:submerged_in",
				"fluid": "water"
			},
			{
				"type": "origins:status_effect",
				"effect": "water_breathing",
				"inverted": true
			},
			{
				"type": "origins:air",
				"comparison": ">",
				"compare_to": 0
			}
			]
		},
		"entity_action": {
			"type": "origins:if_else_list",
			"actions": [
			{
				"condition": {
					"type": "origins:enchantment",
					"enchantment": "respiration",
					"calculation": "max",
					"comparison": "==",
					"compare_to": 0
				},
				"action": {
					"type": "origins:gain_air",
					"value": -10
				}
			},
			{
				"condition": {
					"type": "origins:enchantment",
					"enchantment": "respiration",
					"calculation": "max",
					"comparison": "==",
					"compare_to": 1
				},
				"action": {
					"type": "origins:gain_air",
					"value": -5
				}
			},
			{
				"condition": {
					"type": "origins:enchantment",
					"enchantment": "respiration",
					"calculation": "max",
					"comparison": "==",
					"compare_to": 2
				},
				"action": {
					"type": "origins:gain_air",
					"value": -3
				}
			},
			{
				"condition": {
					"type": "origins:enchantment",
					"enchantment": "respiration",
					"calculation": "max",
					"comparison": "==",
					"compare_to": 3
				},
				"action": {
					"type": "origins:gain_air",
					"value": -2
				}
			}
			]
		}
	}
}